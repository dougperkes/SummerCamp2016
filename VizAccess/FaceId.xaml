<Window x:Class="VizAccess.FaceId"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VizAccess"
        mc:Ignorable="d"
        Title="FaceId" Height="600" Width="600">
    <Grid>
        <Button x:Name="button" Content="Get Face IDs" HorizontalAlignment="Left" Margin="16,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="_logTextBox" HorizontalAlignment="Left" Height="395" Margin="21,41,0,0" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" IsReadOnly="True" VerticalAlignment="Top" Width="564" BorderBrush="White"/>

    </Grid>
</Window>
